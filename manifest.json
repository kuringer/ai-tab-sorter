{
  "manifest_version": 3,
  "name": "AI Tab Sorter",
  "version": "0.1.0",
  "description": "Sorts and groups browser tabs using AI.",
  "permissions": [
    "storage",
    "tabs",
    "tabGroups",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "js/background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}